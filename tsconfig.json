{
  "compilerOptions": {
    "target": "ES2018",
    "allowJs": true,
    "module": "commonjs",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "outDir": "dist",
    "declaration": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "types": [],
    "baseUrl": "."
  },
  "include": ["src"]
}
